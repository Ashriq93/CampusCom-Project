@model IEnumerable<CampusCom.Models.COMMUNITY>

@{
    ViewBag.Title = "Index";
    ViewBag.Header = "CREATION SUCCEEDED!";
    ViewBag.Subheader = "Congratulations, well done!";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComDescr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Privacy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComImg)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ComName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ComDescr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Privacy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ComStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ComImg)
        </td>
        <td>
            
        </td>
    </tr>
}

</table>
